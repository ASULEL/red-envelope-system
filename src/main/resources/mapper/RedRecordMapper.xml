        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asule.redenvelopesystem.mapper.RedRecordMapper">

    <resultMap id="BaseResultMap" type="com.asule.redenvelopesystem.domain.RedRecord">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="userId" column="user_id" jdbcType="VARCHAR"/>
